<Window x:Class="Uchebka.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Uchebka"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Frame x:Name="DataFrame" Grid.Row="1" Grid.ColumnSpan="3"/>
        <Button Grid.Column="0" Content="Основная таблица" Click="Button_Click"/>
        <Button Grid.Column="1" Content="Вторая таблица" Click="Button_Click_1"/>
        <Button Grid.Column="2" Content="Третья страница" Click="Button_Click_2"/>
    </Grid>
</Window>
